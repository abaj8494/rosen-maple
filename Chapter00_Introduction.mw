<Output><Text-field style="2D Output" layout="Maple Output"><Equation executable="false" style="2D Output" display="">NiUiIioiIzUiIzY=</Equation></Text-field>
</Output>
</Group>
<Group hide-input="false" labelreference="L275" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"><Equation executable="true" style="2D Input" input-equation="" display=""></Equation></Text-field>
</Input>
<Output><Text-field style="2D Output" layout="Maple Output"><Equation executable="false" style="2D Output" display="">NiUiIikiIioiIzU=</Equation></Text-field>
</Output>
