<Group view="presentation" hide-input="false" hide-output="true" inline-output="false" labelreference="L587" drawlabel="true">
<Input><Text-field style="NormalNewpar" layout="NormalNewpar">Here is the implementation of <Font style="Maple Input" executable="false">merge</Font>.</Text-field>
</Input>
</Group></Presentation-Block><Presentation-Block><CodeEditor-ExecGroup autoexecute="true" view="presentation" inline-output="false" labelreference="L1131" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion15" expanded="true" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true">merge := proc(l1::list(integer),l2::list(integer))
	local L, L1, L2;
	L1 := l1;
	L2 := l2;
	L := [];
	while L1 &lt;&gt; [] and L2 &lt;&gt; [] do
		if L1[1] &lt; L2[1] then
			L := [op(L),L1[1]];
			L1 := L1[2..-1];
		else
			L := [op(L),L2[1]];
			L2 := L2[2..-1];
		end if;
		if L1 = [] then
			L := [op(L),op(L2)];
			L2 := [];
		elif L2 = [] then
			L := [op(L),op(L1)];
			L1 := [];
		end if;
	end do;
	return L;
end proc:</EC-CodeEditor></CodeEditor-ExecGroup></Presentation-Block><Presentation-Block>
