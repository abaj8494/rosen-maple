<Output><Text-field style="Line Printed Output" layout="Line Printed Output">
QuineMcCluskey := proc(F, variables)
local Fbits, FbitsL, coverageDict, groups, primes, i, allprimeImps, j, coverageTable, essentialPIs, minBits, nextPI;
     |Calls Seconds  Words|
PROC | 1000   0.903 11316259|
   1 | 1000   0.167 1769082| Fbits := DNFtoBitList(F,variables);
   2 | 1000   0.008 361818| coverageDict := initCoverDict(Fbits);
   3 | 1000   0.002      0| i := 0;
   4 | 1000   0.124 273355| FbitsL[0] := Fbits;
   5 | 1000   0.019  14080| while FbitsL[i] &lt;&gt; [] do
   6 | 1816   0.001      0|     i := i+1;
   7 | 1816   0.263 5184072|     groups[i] := sortGroups(FbitsL[i-1]);
   8 | 1816   0.056 898200|     FbitsL[i], primes[i] := NextBitList(groups[i],coverageDict)
                            end do;
   9 | 1000   0.001      0| allprimeImps := {};
  10 | 1000   0.002      0| for j to i do
  11 | 1816   0.008  31134|     allprimeImps := allprimeImps union primes[j]
                            end do;
  12 | 1000   0.005   4000| allprimeImps := [op(allprimeImps)];
  13 | 1000   0.037 554955| coverageTable := initCoverMatrix(Fbits,allprimeImps,coverageDict);
  14 | 1000   0.042 330035| essentialPIs := FindEssentials(coverageTable);
  15 | 1000   0.000      0| minBits := [];
  16 | 1000   0.006      0| for i from nops(essentialPIs) by -1 to 1 do
  17 | 4187   0.088 961581|     minBits := [op(minBits), UpdateCT(essentialPIs[i],coverageTable,Fbits,allprimeImps)]
                            end do;
  18 | 1000   0.003  20100| while 0 &lt; LinearAlgebra[LinearAlgebra:-ColumnDimension](coverageTable) do
  19 |    5   0.000    430|     nextPI := findBestImp(coverageTable);
  20 |    5   0.000    895|     minBits := [op(minBits), UpdateCT(nextPI,coverageTable,Fbits,allprimeImps)]
                            end do;
  21 | 1000   0.071 912522| return Logic:-`&amp;or`(op(map(BitStringtoMT,minBits,variables)))
end proc
</Text-field>
</Output>
</Group></Presentation-Block><Presentation-Block>
